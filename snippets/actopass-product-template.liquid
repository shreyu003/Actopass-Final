        <div class="common-box-for-border">
          {% for block in section.blocks %}
          {% case block.type %}
          {% when 'extra-info' %}
          <div class="accordion accordion-flush" id="accordionFlushExample">
            <div class="accordioncontainer">
              <div id="accordion-{{ block.id }}">
                <div class="head accordian_header">
                  <div>
                    <h3>{{ block.settings.accor_heading }}</h3>
                    <p>{{ block.settings.accor_subheading }}</p>
                  </div>
                  {% render 'asAngleTopIcon' %}
                </div>
                <div class="highlightcontent">
                  <div> {{ block.settings.accor_desc }} </div>
                </div>
              </div>
            </div>
          </div>
          {% endcase %}
          {% endfor %}
        </div>

        <div class="loaction_block">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'loaction-block' %}
                <h2>{{ block.settings.loaction-ttl }}</h2>
                <div class="locationcity">
                  <span> {{ block.settings.loaction-icon }} </span>
                  <h3>{{ block.settings.city-name }}</h3>
                </div>
                <div class="loactionfull">
                  <p> {{ block.settings.address_txt }} </p>
                </div>
                <div class="locationgmap">
                  {{ block.settings.loaction-map }}
                </div>
            {% endcase %}
          {% endfor %}
        </div>
      </div>
      </div>



      <div class="eventSectionTwo">
        <div class="eventsPricestart">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'price-detail' %}
                <h2 class="event-price-heading">{{ block.settings.price-from_ttl }}</h2>
                <div class="bookingDiv">
