                    <h2>{{ product.price | money_with_currency }}</h2>
                    <h3><s>{{ product.compare_at_price | money_with_currency }}</s></h3>
                    <h4>
                      <span> {{ block.settings.instant_confirmation-icon }} </span>
                      {{ block.settings.icon-txt }}
                    </h4>
                  </div>
                  <div class="bookTickets">
                    <a href="{{ block.settings.select-ticket-btn }}" class="eventtcktselect">
                      {{ block.settings.select-ticket-btn-icon }}
                      {{ block.settings.select-ticket-btn-txt }}
                    </a>
                  </div>
                </div>
            {% endcase %}
          {% endfor %}
